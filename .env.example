# Traveler runtime env (example)
# Copy to .env and fill in values.

# Rote API
ROTE_API_BASE=https://api.rote.ink/v2/api
ROTE_OPENKEY=REPLACE_ME

# Optional: OpenClaw Gateway (if you want Traveler to forward events to OpenClaw)
OPENCLAW_GATEWAY_URL=http://127.0.0.1:18789
OPENCLAW_GATEWAY_TOKEN=REPLACE_ME

# Webhook server security (set at least one)
WEBHOOK_TOKEN=REPLACE_ME
WEBHOOK_SECRET=REPLACE_ME
