# Traveler receiver env (example)
# Copy to .env and fill in values.

PORT=8788
WEBHOOK_PATH=/webhook

# Optional shared secret for HMAC verification.
# If set, receiver requires X-Signature-256: sha256=<hex(hmac_sha256(secret, raw_body))>
RECEIVER_SHARED_SECRET=REPLACE_ME

# Optional: Forward to OpenClaw Gateway as a systemEvent or sessions_spawn task.
OPENCLAW_GATEWAY_URL=http://127.0.0.1:18789
OPENCLAW_GATEWAY_TOKEN=REPLACE_ME
OPENCLAW_SESSION_KEY=agent:main:main

# Optional: only accept specific event sources (comma-separated). Empty = allow all.
ALLOW_SOURCES=github,custom
